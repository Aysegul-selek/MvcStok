@using MvcStok.Models.Entity
@model List <TblKategoriler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>KATEGORİ ID</th>
        <th>KATEGORİ ADI</th>
    </tr>
    <tbody>
        @foreach(var ktgr in Model)
        {
        <tr>
            <td>@ktgr.KategoriId</td>
            <td>@ktgr.KategoriAd</td>
        </tr>
        }
    </tbody>
</table>
